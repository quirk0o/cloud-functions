{
  "name": "@quirk0.o/serverless-research-azure",
  "version": "1.0.1",
  "description": "Azure Functions sample for the Serverless framework",
  "private": true,
  "main": "handler.js",
  "keywords": [
    "azure",
    "serverless"
  ],
  "scripts": {
    "deploy": "sls deploy",
    "deploy:prod": "npm-run-all \"deploy -s prod {@}\" \"upload ../../sample_files/*  -s prod {@}\" --",
    "deploy:test": "npm-run-all \"deploy -s test {@}\" \"upload ../../sample_files/1MB.dat -s test {@}\" --",
    "test": "yarn geturl -s test | xargs yarn invoke",
    "invoke": "curl -X POST -H 'cache-control: no-cache' -H 'content-type: application/json' -d '{\"size\": \"1MB.dat\"}'",
    "invoke:dev": "yarn geturl | xargs yarn invoke",
    "invoke:prod": "yarn geturl -s prod | xargs yarn invoke",
    "upload": "node tools/upload.js",
    "setenv": "node tools/setenv.js",
    "geturl": "node tools/geturl.js"
  },
  "devDependencies": {
    "config": "^1.30.0",
    "js-yaml": "^3.12.0",
    "npm-run-all": "^4.1.3",
    "serverless-azure-functions": "*",
    "serverless-plugin-write-env-vars": "^1.0.2",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "@quirk0.o/async": "^1.0.1",
    "@quirk0.o/benchmark": "^1.0.1",
    "@quirk0.o/cloud-functions-common": "2.0.4",
    "@quirk0.o/random-stream": "^2.0.0",
    "azure-storage": "^2.8.2",
    "bluebird": "^3.5.0",
    "bytes": "^3.0.0",
    "dotenv": "^6.0.0"
  },
  "peerDependencies": {
    "serverless": "^1.27.2"
  }
}
